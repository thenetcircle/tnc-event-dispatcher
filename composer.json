{
  "name": "tnc/EventDispatcherService",
  "description": "Dispatchs and listens to events, Which expends symfony/event-dispatcher to support asynchronous event dispatching and listening.",
  "keywords": ["event", "event dispatcher"],
  "type": "library",
  "license": "proprietary",

  "require": {
    "php": ">=5.6",
    "symfony/event-dispatcher": "~2.3 || ~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "kwn/php-rdkafka-stubs": "~0.0.4"
  },

  "autoload" : {
    "psr-4" : { "Tnc\\Service\\EventDispatcher\\" : "src/" }
  },
  "autoload-dev" : {
    "psr-4" : { "Tnc\\Service\\EventDispatcher\\Tests\\" : "tests/" }
  }
}
