{
  "name": "tnc/event-dispatcher",
  "description": "A alternative async EventDispatcher, based on Symfony EventDispatcher",
  "type": "library",
  "license": "Apache-2.0",
  "require": {
    "symfony/event-dispatcher": "~2.3 || ~3.0"
  },
  "require-dev": {
    "psr/log": "~1.0",
    "phpunit/phpunit": "~4.0||~5.0||~6.0",
    "guzzlehttp/guzzle": "~6.0",
    "psr/http-message": "^1.0"
  },
  "suggest": {
    "guzzlehttp/guzzle": "EventBusEndPoint requires it.",
    "psr/http-message": "EventBusReceiver requires it."
  },
  "autoload": {
    "psr-4": {
      "TNC\\EventDispatcher\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TNC\\EventDispatcher\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "sort-packages": true
  }
}
