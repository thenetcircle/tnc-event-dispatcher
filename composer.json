{
  "name": "tnc/service-eventdispatcher",
  "description": "Dispatchs and listens to events, Which expends symfony/event-dispatcher to support asynchronous event dispatching and listening.",
  "keywords": [
    "event",
    "event dispatcher"
  ],
  "type": "library",
  "license": "proprietary",
  "require": {
    "php": ">=5.5",
    "misterion/ko-process": "*"
  },
  "require-dev": {
    "psr/log": "~1.0",
    "phpunit/phpunit": "~5.0",
    "kwn/php-rdkafka-stubs": "~0.0.4",
    "symfony/event-dispatcher": "~2.3 || ~3.0"
  },
  "autoload": {
    "psr-4": {
      "Tnc\\Service\\EventDispatcher\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tnc\\Service\\EventDispatcher\\Tests\\": "tests/"
    }
  }
}
